synonym: "dextro-sinister axis" EXACT []
synonym: "L-R axis" EXACT []
synonym: "left to right axis" EXACT []
synonym: "LR axis" EXACT []
synonym: "R-L axis" EXACT []
synonym: "right to left axis" EXACT []
